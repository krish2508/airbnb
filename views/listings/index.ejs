<% layout("/layouts/boilerplate") %>
<body>
    <h1>All listings</h1>
    <div class="row row-cols-lg-4 row-cols-md-3 row-cols-sm-2">
    <% for(let list of alllistings){ %>
    <a href="/listings/<%= list.id %>" style="text-decoration: none;">
    <div class="card " style="width: 18rem;">
        <img src="<%= list.image %>" style="height: 20rem;"class="card-img-top" alt="listing_image">
        <div class="card-body">
          <p class="card-text"><%= list.title %> <br>&#8377;<%=  list.price.toLocaleString("en-IN") %></p>        </div>
      </div>
    </a>
      <% } %>
    </div>
</body>
